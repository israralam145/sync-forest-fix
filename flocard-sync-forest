                function display() {
                    compactIDB.readAllData("clusters").then(resp => {
                        var keys = Object.keys(resp);
                        //totalElementCount = keys;
                        //console.log(totalElementCount);
                        var totalLocalCnt = document.getElementById('totalLocal');
                        var synced = 0;
                        var pending = 0;

                        var html = "";
                        var synced = "";
                        var sl = 1;
                        for (var key = keys.length; key >= 1; key--) {
                            var clr = "#00d179";
                            var status = "Synced";
                            if (resp[key]["sync_flag"] == "false") {
                                pending++;
                                clr = "red";
                                status = "Pending";
                            }
                            else {
                                synced++;
                            }

                            var file = resp[key]["img"];
                            var url = URL.createObjectURL(file);

                            var tag_ts = new Date(resp[key]["tagged_ts"]);

                            tag_ts = tag_ts.getFullYear() + "-" + (((tag_ts.getMonth() + 1) < 10) ? "0" : "") + (tag_ts.getMonth() + 1) + "-" + ((tag_ts.getDate() < 10) ? "0" : "") + tag_ts.getDate() + " " + ((tag_ts.getHours() < 10) ? "0" : "") + tag_ts.getHours() + ":" + ((tag_ts.getMinutes() < 10) ? "0" : "") + tag_ts.getMinutes() + ":" + ((tag_ts.getSeconds() < 10) ? "0" : "") + tag_ts.getSeconds();


                            html += '<tr id="' + key + '"><td>' + sl + '</td><td><div style = "width:100%; border-radius: 14px;" > <img class="img-fluid" src = ' + url + ' alt = "Asset image" ></div></td><td>' + resp[key]["cluster_name"] + '</td><td>' + tag_ts + '</td><td align="center"><a href="https://maps.google.com/?q=' + resp[key]["lat"] + ',' + resp[key]["long"] + '" target="_blank" title="Location" style="color:#00D179" class="fa fa-map-marker"></a></td><td></td><td></td><td><span class="tio-upload-on-cloud tio-xl" style = "color: ' + clr + '; border-color:' + clr + ';" > </span></td><td style="display:none">' + status + '</td></tr>';
                            totalElementCount++;

                            totalElementCount++;
                            sl++;
                        };
                        //console.log(totalElementCount);
                        $("#tblData").empty();
                        $("#tblData").append(html);
                        totalLocalCnt.innerHTML = pending + " Forests to be Synced";

                        initDatatable();
                    });
                }

                async function sync() {
                    console.log("Sync Started");
                    var primkeys = [];
                    await compactIDB.readAllData("clusters").then(resp => {
                        var keys = Object.keys(resp);

                        //totalElementCount = keys;
                        //console.log(totalElementCount);
                        var html = "";
                        for (var key of keys) {
                            debugger;
                            var file = resp[key]["img"];
                            var cluster_name = resp[key]["cluster_name"];
                            var area = resp[key]["area"];
                            var perimeter = resp[key]["perimeter"];
                            var lat = resp[key]["lat"];
                            var long = resp[key]["long"];
                            var coordinates = resp[key]["coordinates"];

                            var success = false;
                            if (file) {
                                var formData = new FormData();
                                formData.append('image', file);
                                formData.append('Cluster_Name', cluster_name);
                                formData.append('Area', area);
                                formData.append('Perimeter', perimeter);
                                formData.append('Latitude', lat);
                                formData.append('Longitude', long);
                                formData.append('Points', JSON.stringify(coordinates));
                                $.ajax({
                                    url: '/User/AddCluster',
                                    type: 'POST',
                                    async: false,
                                    data: formData,
                                    processData: false,
                                    contentType: false,
                                    success: function (response) {
                                        //Handle success response
                                        debugger;
                                        if (response.status == 1) {
                                            console.log(response);
                                            console.log("Cluster synced with key " + key);
                                            primkeys.push(key);
                                            //compactIDB.removeData("clusters", parseInt(key)).then(resp => {
                                            //    console.log(resp);
                                            //    display();
                                            //});
                                            compactIDB.updateFlag("clusters", "true", parseInt(key)).then(resp => {
                                                console.log(resp);
                                                display();
                                            });
                                        }
                                        else {
                                            console.log(response);
                                            console.log("Error Uploading Image for key " + key);
                                        }

                                    },
                                    error: function (xhr, status, error) {
                                        // Handle error response
                                    }
                                });
                            }
                            else {
                                console.log("Sync skipped as no image for key " + key);
                            }
                        }

                        alert("Sync Completed");
                        location.reload(true);

                    });



                }


                const syncEventListner = document.getElementById('btnSync');
                syncEventListner.addEventListener("click", sync);
                const IndexedDBName = "Flocard";
                const TableName = "clusters";
                const TagsHTMLTableID = "tags-table";
                //const syncAPI = "/User/PushCluster";
                const syncAPI = "/User/PushCluster";
                const syncAPIMethod = "POST";
                const invokeInIndexedDB = (actionFn) => {
                    const request = window.indexedDB.open(IndexedDBName);
                    request.onerror = (e) => {
                        console.log("IndexedDB error: ", e);
                        //alert("Something went wrong with local database. Open console for details.");
                    };
                    request.onsuccess = ({ target }) => {
                        const { result: db } = target
                        actionFn(db);
                    };
                }
                const invokeForTagsRow = (actionFn) => (db) => {
                    const transaction = db.transaction(TableName);
                    const objectStore = transaction.objectStore(TableName);
                    objectStore.openCursor().onsuccess = ({ target }) => {
                        const { result: cursor } = target;
                        const { primaryKey, value } = cursor || {};
                        if (cursor) {
                            actionFn({ db, primaryKey, value });
                            cursor.continue();
                        }
                    }
                }
                const removeDBTagRow = ({ onSuccessCallback, onErrorCallback }) => ({ db, ...metaData }) => {
                    const { primaryKey } = metaData;
                    const transaction = db.transaction([TableName], "readwrite");
                    const objectStore = transaction.objectStore(TableName);
                    const request = objectStore.delete(primaryKey);
                    request.onsuccess = () => onSuccessCallback(metaData);
                    request.onerror = () => onErrorCallback(metaData);
                }

                const removeHTMLTagRow = (metaData) => {
                    const { primaryKey, requestPayload: { ddlSpecies } } = metaData;
                    const rowID = ddlSpecies + primaryKey;
                    const tableHTML = document.getElementById(TagsHTMLTableID);
                    const rowHTML = document.getElementById(rowID);
                    totalSuccessCount++;
                    if (tableHTML && rowHTML) tableHTML.deleteRow(rowHTML.rowIndex);
                    const isTableEmpty = tableHTML.getElementsByTagName('tr').length === 1;
                    if (isTableEmpty) { promtCompletion(); };
                    return metaData;
                }

                function promtCompletion() {
                    setTimeout(300);
                    if (totalElementCount == totalSuccessCount) {

                        $("#displayProgressPercentage").hide();

                        var goToOfflineData = document.getElementById("offlineData");
                        var goToEditAsset = document.getElementById("gotToEditAsset");
                        var retVal = confirm("All " + totalElementCount + " items have been synced.");
                        location.reload(true);
                        //if (retVal == true) {
                        //    goToEditAsset.click();
                        //}
                        //else {
                        //    goToOfflineData.click();
                        //}
                    }
                    else if (totalElementCount == totalFailureCount) {
                        //alert("Items could not be synced, try again with better internet connection.");

                        $("#displayProgressPercentage").hide();

                        var goToOfflineData = document.getElementById("offlineData");
                        var goToEditAsset = document.getElementById("gotToEditAsset");
                        var retVal = confirm("Items could not be synced, try again with better internet connection.");
                        location.reload(true);
                        //if (retVal == true) {
                        //    goToEditAsset.click();
                        //}
                        //else {
                        //    goToOfflineData.click();
                        //}
                    }
                    else if (totalElementCount == (totalFailureCount + totalSuccessCount)) {
                        //alert("Total items synced:" +totalSuccessCount+ ". Total failed item:" +totalFailureCount);

                        $("#displayProgressPercentage").hide();

                        var goToOfflineData = document.getElementById("offlineData");
                        var goToEditAsset = document.getElementById("gotToEditAsset");
                        var retVal = confirm("Total items synced:" + totalSuccessCount + ". Total failed item:" + totalFailureCount);
                        location.reload(true);
                        //if (retVal == true) {
                        //    goToEditAsset.click();
                        //}
                        //else {
                        //    goToOfflineData.click();
                        //}
                    }

                }



                const applyFns = fns => x => fns.reduce((resp, fn) => resp ? fn(resp) : undefined, x);
                const prepareTagData = ({ value, ...rest }) => {
                    const { species: ddlSpecies, otherName: txtOther, lat: txtLat, long: txtLong, date: txtDoP, imageVal: txtImg } = value;
                    return { ...rest, payload: { ddlSpecies, txtOther, txtLat, txtLong, txtImg, txtDoP } };
                }

                const validateTagData = (preppedData) => {
                    const { payload } = preppedData;
                    const { txtLat, txtLong, txtImg } = payload;
                    if (!txtLat || !txtLong || !txtImg) {
                        alert("Latitude/Longitude/Image is missing/incorrect.");
                        //$("#spinnerID").hide();
                        $("#displayProgressPercentage").hide();
                        return undefined;
                    }
                    return preppedData;
                }

                const createFormData = (data) => {
                    const { payload, ...rest } = data;
                    const formData = new FormData();
                    Object.keys(payload).forEach(key => formData.append(key, payload[key]))
                    return { ...rest, formData };
                }

                const pushTagData = ({ onSuccessCallback, onFailureCallback }) => ({ formData, ...rest }) => {
                    const request = new XMLHttpRequest();
                    request.open(syncAPIMethod, syncAPI);
                    request.onreadystatechange = ({ target }) => {
                        const { readyState, status } = target;
                        //console.log(target);
                        const details = { ...rest, requestPayload: Object.fromEntries(formData), response: target };
                        if (readyState === XMLHttpRequest.DONE) {
                            switch (status) {
                                case 200: progressPercentage(); onSuccessCallback(details); break;
                                default: return (onFailureCallback(details));
                            }
                        }
                    }
                    const maybePromise = request.send(formData)
                    if (maybePromise) maybePromise.then(removeDisableButton).catch(removeDisableButton);
                }



                const handleLocalUpdateFailure = e => {
                    totalFailureCount++;
                    //alert("Local Update Error Occured. Open console to for details");
                    promtCompletion();
                    console.log("Local Update Error for the following meta: ", e);
                    //$("#displayProgressPercentage").hide();
                    return e;
                }

                const handleSyncSuccess = removeDBTagRow({ onSuccessCallback: removeHTMLTagRow, onFailureCallback: handleLocalUpdateFailure })
                const handleSyncFailure = e => {
                    totalFailureCount++;
                    promtCompletion();
                    //alert("Sync Error Occured. Open console to for details");
                    console.log("Sync Error for the following meta: ", e);
                    $("#displayProgressPercentage").hide();
                    return e;
                }

                function progressPercentage() {
                    console.log(totalElementCount);
                    debugger;
                    var percent = (totalSuccessCount / totalElementCount) * 100;
                    if (percent == 0) {
                        percent = 1;
                    }
                    var displayPercent = 100 - percent;
                    console.log(percent);
                    document.getElementById('progressBarPercentage').style.width = displayPercent + '%';
                }

                async function SyncDataBase() {
                    let tableBody = document.getElementById("tblData")
                    if (tableBody.rows.length === 0) {
                        alert("No data to Sync")
                    } else {
                        //var element = document.getElementById('spinnerID');
                        var element = document.getElementById('displayProgressPercentage');
                        debugger;
                        var button = $('#btnSync');
                        $(button).attr('disabled', 'disabled');
                        element.style.removeProperty("visibility");
                        const postTagData = pushTagData({ onSuccessCallback: handleSyncSuccess, onFailureCallback: handleSyncFailure });
                        const actions = [prepareTagData, validateTagData, createFormData, postTagData];
                        const publishTag = applyFns(actions);
                        invokeInIndexedDB(invokeForTagsRow(publishTag));
                    }
                    //$(button).removeAttr('disabled');
                }

                function goToEditPageBtn() {
                    var url = '/User/SyncCluster';
                    window.location.href = url;
                }

            </script>


        </div>
        <!-- End Content -->
        <!-- Footer -->

        <div class="footer">
            <div class="row justify-content-between align-items-center">
            </div>
        </div>



        <!-- End Footer -->
        <script>
            var groupColumn = 2;
            var table = $('#datatable').DataTable({
                columnDefs: [{ visible: false, targets: groupColumn }],
                order: [[groupColumn, 'asc']],
                displayLength: 25,
                drawCallback: function (settings) {
                    var api = this.api();
                    var rows = api.rows({ page: 'current' }).nodes();
                    var last = null;

                    api.column(groupColumn, { page: 'current' })
                        .data()
                        .each(function (group, i) {
                            if (last !== group) {
                                $(rows)
                                    .eq(i)
                                    .before(
                                        '<tr class="group"><td colspan="8">' +
                                        group +
                                        '</td></tr>'
                                    );

                                last = group;
                            }
                        });
                }
            });

            // Order by the grouping
            $('#datatable tbody').on('click', 'tr.group', function () {
                var currentOrder = table.order()[0];
                if (currentOrder[0] === groupColumn && currentOrder[1] === 'asc') {
                    table.order([groupColumn, 'desc']).draw();
                }
                else {
                    table.order([groupColumn, 'asc']).draw();
                }
            });
        </script>
    </main>
