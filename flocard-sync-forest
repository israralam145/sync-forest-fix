                                <script>
                                    const ThemeBTN = document.getElementById("otherLinksDropdown");
                                    const link = document.getElementById("styleThemes");
                                    const themeicon = document.getElementById("themeIcon");
                                    const saved_theme = getCookie("currentTheme");

                                    if (saved_theme === "light") {
                                        link.setAttribute("href", "/assets/css/theme.min.css");
                                        themeicon.setAttribute("class", "fas fa-sun")
                                    } else {
                                        link.setAttribute("href", "/assets/css/theme.dark.min.css");
                                        themeicon.setAttribute("class", "fas fa-moon")
                                    }

                                    ThemeBTN.addEventListener("click", function () {
                                        if (link.getAttribute("href") === "/assets/css/theme.dark.min.css" && themeicon.getAttribute("class") === "fas fa-moon") {
                                            link.setAttribute("href", "/assets/css/theme.min.css");
                                            themeicon.setAttribute("class", "fas fa-sun")
                                            setCookie("currentTheme", "light", 7);
                                        } else {
                                            link.setAttribute("href", "/assets/css/theme.dark.min.css");
                                            themeicon.setAttribute("class", "fas fa-moon");
                                            setCookie("currentTheme", "dark", 7);
                                        }
                                    });

                                    //if (saved_theme === "dark") {
                                    //    link.setAttribute("href", "/assets/css/theme.dark.min.css");
                                    //    themeicon.setAttribute("class", "fas fa-moon");

                                    //} else {
                                    //    link.setAttribute("href", "/assets/css/theme.min.css");
                                    //    themeicon.setAttribute("class", "fas fa-sun");

                                    //}

                                    //ThemeBTN.addEventListener("click", function () {
                                    //    if (link.getAttribute("href") === "/assets/css/theme.min.css" && themeicon.getAttribute("class") === "fas fa-sun") {
                                    //        link.setAttribute("href", "/assets/css/theme.dark.min.css");
                                    //        themeicon.setAttribute("class", "fas fa-moon")
                                    //        setCookie("currentTheme", "dark", 7);


                                    //    } else {
                                    //        link.setAttribute("href", "/assets/css/theme.min.css");
                                    //        themeicon.setAttribute("class", "fas fa-sun");
                                    //        setCookie("currentTheme", "light", 7);

                                    //    }
                                    //});

                                    function setCookie(name, value, days) {
                                        let expires = "";
                                        if (days) {
                                            const date = new Date();
                                            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                                            expires = "; expires=" + date.toUTCString();
                                        }
                                        document.cookie = name + "=" + (value || "") + expires + "; path=/";
                                    }

                                    function getCookie(name) {
                                        const value = "; " + document.cookie;
                                        const parts = value.split("; " + name + "=");
                                        if (parts.length === 2) {
                                            return parts.pop().split(";").shift();
                                        }
                                    }

                                </script>
                            </div>

