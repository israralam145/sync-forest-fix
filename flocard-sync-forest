        $(document).on('ready', function () {

            $("#lnkFloCard").on('click', function (event) {
                var url = "/User/FlocardSignin/";

                $.get(url, function (resp) {
                    if (resp.status == 1) {
                        window.location.href = resp.redirect_uri;
                    }
                    else {
                        alert("Error Redirecting to FloCard.");
                        console.log("FloCard Redirection Error : " + resp.message);
                    }
                });

            });




